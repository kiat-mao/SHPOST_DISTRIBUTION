<%= form_for @commodity, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :cno, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :cno, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :dms_no, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :dms_no, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :name, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :supplier_id, :class => 'control-label' %>
    <div class="controls">
      <%= supplier_select_autocom("commodity",@commodity.id) %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :cost_price, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :cost_price, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :sell_price, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :sell_price, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :desc, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_area :desc, :class => 'text_field' %>
    </div>
  </div>
  
  <%= f.submit nil, :class => 'btn btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
            commodities_path, :class => 'btn btn-default' %>
 
<% end %>


